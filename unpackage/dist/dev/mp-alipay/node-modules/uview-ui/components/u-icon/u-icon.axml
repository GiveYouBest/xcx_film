<view data-event-opts="{{[['tap',[['click',['$event']]]]]}}" class="{{(('u-icon data-v-ad5282ee')+' '+('u-icon--'+labelPos))}}" style="{{$root.s0}}" onTap="__e"><block a:if="{{isImg}}"><image class="u-icon__img data-v-ad5282ee" style="{{$root.s1}}" src="{{name}}" mode="{{imgMode}}"></image></block><block a:else><text class="{{(('u-icon__icon data-v-ad5282ee')+' '+customClass)}}" style="{{$root.s2}}" hover-class="{{hoverClass}}" data-event-opts="{{[['touchStart',[['touchstart',['$event']]]]]}}" onTouchStart="__e"></text></block><block a:if="{{label}}"><text class="u-icon__label data-v-ad5282ee" style="{{'color:'+(labelColor)+';'+('font-size:'+(labelSize+'rpx')+';')+('margin-left:'+(labelPos=='right'?marginLeft+'rpx':0)+';')+('margin-top:'+(labelPos=='bottom'?marginTop+'rpx':0)+';')+('margin-right:'+(labelPos=='left'?marginRight+'rpx':0)+';')+('margin-bottom:'+(labelPos=='top'?marginBottom+'rpx':0)+';')}}">{{label}}</text></block></view>